{
  "name": "samui-samui-de",
  "slug": "samui-samui-de",
  "url": "https://samui-samui.de",
  "version": "2020.0.0",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "description": "Hugo files for samui-samui.de",
  "homepage": "http://samui-samui.de",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/pkollitsch/samui-samui.de.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "scripts": {
    "algolia2": "atomic-algolia",
    "algolia": "hugo --quiet && atomic-algolia && rm -rf public",
    "lighthouse": "lighthouse https://samui-samui.de --budget-path=budget.json --view",
    "release": "standard-version --release-as patch -a -t \"\"",
    "release-next": "standard-version --release-as minor -a -t \"\"",
    "release-major": "standard-version --release-as major -a -t \"\""
  },
  "browserslist": [
    "> 1%",
    "last 2 version"
  ],
  "dependencies": {
    "atomic-algolia": "0.3.19",
    "autoprefixer": "9.8.6",
    "bootstrap": "4.5.2",
    "caniuse-lite": "1.0.30001132",
    "cookieconsent": "3.1.1",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "6.3.0",
    "imagemin-cli": "6.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-pngquant": "9.0.1",
    "instantsearch.js": "4.8.1",
    "jquery": "3.5.1",
    "lodash": "4.17.20",
    "minimist": "1.2.5",
    "moment": "2.28.0",
    "popper.js": "1.16.1",
    "postcss-cli": "7.1.2",
    "standard-version": "^9.0.0"
  },
  "standard-version": {
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ]
  }
}
