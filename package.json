{
  "name": "samui-samui-de",
  "slug": "samui-samui-de",
  "url": "https://samui-samui.de",
  "version": "1.1.27",
  "author": {
    "name": "Patrick Kollitsch",
    "email": "patrick@davids-neighbour.com",
    "web": "https://davids-neighbour.com"
  },
  "description": "Website setup for samui-samui.de",
  "homepage": "https://samui-samui.de",
  "license": "UNLICENSED",
  "private": true,
  "repository": "davidsneighbour/samui-samui.de",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@fullhuman/postcss-purgecss": "3.0.0",
    "atomic-algolia": "0.3.19",
    "autoprefixer": "10.1.0",
    "cli-real-favicon": "0.0.8",
    "commitizen": "4.2.2",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "6.3.0",
    "cz-customizable-ghooks": "2.0.0",
    "husky": "4.3.5",
    "lighthouse": "6.5.0",
    "netlify-plugin-snyk": "1.2.1",
    "pixrem": "5.0.0",
    "playwright": "1.6.2",
    "postcss": "8.2.1",
    "postcss-cli": "^8.0.0",
    "precss": "4.0.0",
    "remark-preset-lint-dnb": "github:dnb-hugo/remark-preset-lint-dnb",
    "standard-version": "9.0.0"
  },
  "scripts": {
    "serve": "run-p start:**",
    "serve:hugo": "hugo server --minify --disableFastRender --i18n-warnings --navigateToChanged  --templateMetrics --templateMetricsHints --verbose --verboseLog --path-warnings --buildDrafts --buildExpired --buildFuture --watch --enableGitInfo --forceSyncStatic --log true --logFile hugo.log --verbose",
    "serve:webpack": "webpack-dev-server --config webpack.dev.js --hot",
    "algolia": "hugo --quiet && atomic-algolia && rm -rf public",
    "preversion": "if [ \"$(git rev-parse --abbrev-ref HEAD)\" = main ] ; then echo Preversion checks passed. ; else echo Error: Not on branch release. ; exit 1 ; fi",
    "release": "standard-version --release-as patch -a -t \"v\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"v\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"v\" && ./bin/release-hook-postrelease.sh",
    "commitlint": "commitlint --from=HEAD~1",
    "favicon-create": "real-favicon generate faviconDescription.json ./resources/_gen/assets/faviconData.json static/images/favicon/",
    "favicon-update": "real-favicon check-for-update --fail-on-update ./resources/_gen/assets/faviconData.json",
    "lint-md": "remark --ignore-path .gitignore content",
    "lint": "eslint src",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site -v",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-syntax-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.12.10",
    "@babel/register": "7.12.1",
    "assets-webpack-plugin": "3.9.7",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "7.0.0",
    "cross-env": "5.2.0",
    "css-loader": "3.5.3",
    "eslint": "7.15.0",
    "eslint-plugin-import": "2.22.1",
    "exports-loader": "0.7.0",
    "fancy-log": "1.3.2",
    "file-loader": "6.2.0",
    "hugo-bin": "0.63.0",
    "imports-loader": "1.2.0",
    "mini-css-extract-plugin": "0.12.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "plugin-error": "1.0.1",
    "postcss-import": "13.0.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "rimraf": "3.0.0",
    "sass-loader": "10.1.0",
    "style-loader": "1.3.0",
    "uglifyjs-webpack-plugin": "2.1.3",
    "url-loader": "4.1.1",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.1.4",
    "whatwg-fetch": "3.5.0"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    },
    "scripts": {
      "prerelease": "./bin/release-hook-prerelease.sh"
    },
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        100
      ],
      "type-enum": [
        0,
        "always",
        [
          "feat",
          "fix",
          "layouts",
          "refactor",
          "test",
          "chore",
          "wip",
          "content"
        ]
      ]
    }
  }
}
