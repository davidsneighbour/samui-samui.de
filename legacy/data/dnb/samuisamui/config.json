  "params": {
    "author": {
      "content": "sitewide/authorfooter/index.md",
      "format": {
        "layout": "card"
      },
      "homepage": "https://kollitsch.de/",
      "name": "Patrick Kollitsch",
      "photo": {
        "name": "patrick-kollitsch.jpg",
        "title": "Der Autor in seiner natürlichen Umgebung"
      }
    },
    "copyright": "All rights reserved. Copyright samui-samui.de",
    "description": "David's Neighbour's Notizen über sein Leben als Auswanderer auf der Insel Koh Samui in Thailand. Auf Deutsch, und so...",
    "dnb": {
      "auditor": {
        "descriptionmaxlength": 160,
        "descriptionminlength": 110,
        "descriptionthreshold": 10,
        "titlemaxlength": 60,
        "titleminlength": 50,
        "titlethreshold": 10
      },
      "comments": {},
      "copy-from": "2005",
      "debug": {
        "config": {
          "plugins": {
            "js": {
              "imports": [
                "import 'debug';"
              ]
            },
            "scss": {
              "imports": [
                "scss/_debugprint.scss"
              ]
            }
          }
        },
        "debuglevel": 8,
        "development": true,
        "formats": {},
        "loglink": "https://dnbql.netlify.app/log",
        "namespace": "dnb"
      },
      "feeds": {
        "atom": {
          "limit": 10,
          "summary": true
        },
        "json": {
          "limit": 10,
          "summary": true
        },
        "limit": 10,
        "rss": {
          "limit": 10,
          "summary": true,
          "webfeed": {
            "analytics": {
              "engine": "GoogleAnalytics"
            }
          }
        },
        "summary": true
      },
      "footer": {
        "index": {
          "enabled": true,
          "items": [
            "tagcloud",
            "leutecloud"
          ]
        },
        "leutecloud": {
          "enabled": true,
          "type": "cloud",
          "values": "leute"
        },
        "single": {},
        "tagcloud": {
          "enabled": true,
          "type": "cloud",
          "values": "tags"
        }
      },
      "forms": {
        "contactform": {
          "attributes": {
            "class": "mb-6",
            "data-netlify": "true",
            "netlify-honeypot": "%random%"
          },
          "buttons": [
            {
              "class": "btn btn-primary",
              "label": "shortcodes.form.submit",
              "name": "shortcodes.form.fieldnames.submit",
              "type": "submit"
            }
          ],
          "fields": {
