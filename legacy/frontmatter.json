{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.defaultFileType": "md",
  "frontMatter.content.defaultSorting": "date",
  "frontMatter.content.fmHighlight": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "Blog Posts",
      "path": "[[workspace]]/content"
    }
  ],
  "frontMatter.content.draftField": {
    "name": "draft",
    "type": "boolean"
  },
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.sorting": [
    {
      "title": "Date (asc)",
      "name": "date",
      "order": "asc",
      "type": "date"
    },
    {
      "title": "Date (desc)",
      "name": "date",
      "order": "desc",
      "type": "date"
    }
  ],
  "frontMatter.content.supportedFileTypes": [
    "md"
  ],
  "frontMatter.content.wysiwyg": true,
  "frontMatter.content.snippet": [
    "{{< caption \"{mediaUrl}\" \"Description\" >}}"
  ],
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.framework.id": "hugo",
  "frontMatter.global.notifications": [
    "info",
    "warning",
    "error"
  ],
  "frontMatter.media.defaultSorting": "FileNameAsc",
  "frontMatter.panel.freeform": true,
  "frontMatter.preview.host": "http://192.168.1.201:1313",
  "frontMatter.preview.pathName": "yyyy/MM",
  "frontMatter.site.baseURL": "https://kollitsch.dev",
  "frontMatter.taxonomy.alignFilename": false,
  "frontMatter.taxonomy.categories": [],
  "frontMatter.taxonomy.commaSeparatedFields": [],
  "frontMatter.taxonomy.dateFormat": "yyyy-MM-dd'T'H:mm:ssxxx",
  "frontMatter.taxonomy.frontMatterType": "YAML",
  "frontMatter.taxonomy.indentArrays": false,
  "frontMatter.taxonomy.noPropertyValueQuotes": [
    "date"
  ],
  "frontMatter.taxonomy.seoContentLength": 1760,
  "frontMatter.taxonomy.seoDescriptionField": "description",
  "frontMatter.taxonomy.seoDescriptionLength": 170,
  "frontMatter.taxonomy.seoSlugLength": 75,
  "frontMatter.taxonomy.seoTitleLength": 60,
  "frontMatter.taxonomy.slugPrefix": "",
  "frontMatter.taxonomy.slugSuffix": "",
  "frontMatter.templates.folder": ".frontmatter/templates",
  "frontMatter.templates.prefix": "",
  "frontMatter.framework.startCommand": "npm run server",
  "frontMatter.file.preserveCasing": false,
  "frontMatter.content.placeholders": [
    {
      "id": "permalink",
      "value": "/blog/{{year}}/{{slug}}/index.md"
    }
  ],
  "frontMatter.templates.enabled": false,
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "previewPath": "{{permalink}}",
      "pageBundle": true,
      "filetype": "md",
      "fieldGroup": [
        "resources"
      ],
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string",
          "single": true
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "default": "{{now}}",
          "isModifiedDate": true
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Resources",
          "name": "resources",
          "type": "block",
          "fieldGroup": [
            "resources"
          ]
        },
        {
          "title": "Weight",
          "name": "weight",
          "type": "number"
        },
        {
          "title": "Boolean",
          "name": "isboolean",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Choice",
          "name": "choice",
          "type": "choice",
          "choices": [
            "",
            "Choice 1",
            "Choice 2",
            "Choice 3"
          ]
        },
        {
          "title": "Choice",
          "name": "choice2",
          "type": "choice",
          "choices": [
            {
              "id": "1",
              "title": "Choice 1"
            },
            {
              "id": "2",
              "title": "Choice 2"
            },
            {
              "id": "3",
              "title": "Choice 3"
            }
          ]
        }
      ]
    }
  ],
  "frontMatter.custom.scripts": [
    {
      "title": "Generate social image for Youtube",
      "script": "./assets/config/frontmatter/scripts/create-youtube-header-image.js",
      "command": "~/.config/nvm/versions/node/v18.0.0/bin/node"
    },
    {
      "title": "Retrieve Unsplash image",
      "script": "./assets/config/frontmatter/scripts/retrieve-unsplash-header-image-info.js",
      "command": "~/.config/nvm/versions/node/v18.0.0/bin/node"
    }
  ],
  "frontMatter.data.files": [
    {
      "id": "music2program2",
      "title": "Music to program to",
      "file": "[[workspace]]/data/dnb/kollitsch/music.json",
      "fileType": "json",
      "labelField": "name",
      "schema": {
        "title": "music2program2",
        "type": "object",
        "required": [
          "name",
          "slug",
          "description",
          "link"
        ],
        "properties": {
          "slug": {
            "type": "string",
            "title": "Slug"
          },
          "name": {
            "type": "string",
            "title": "Name"
          },
          "url": {
            "type": "string",
            "title": "URL"
          },
          "description": {
            "type": "string",
            "title": "Description"
          }
        }
      }
    }
  ],
  "frontMatter.data.folders": [],
  "frontMatter.taxonomy.fieldGroups": [
    {
      "id": "author",
      "labelField": "name",
      "fields": [
        {
          "title": "Author Name",
          "name": "name",
          "type": "string",
          "single": true
        },
        {
          "title": "Social link",
          "name": "social",
          "type": "string",
          "single": true
        }
      ]
    },
    {
      "id": "resources",
      "labelField": "src",
      "fields": [
        {
          "title": "Path/Filename",
          "name": "src",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Name (Identifier)",
          "name": "name",
          "type": "string",
          "single": true
        }
      ]
    }
  ]
}